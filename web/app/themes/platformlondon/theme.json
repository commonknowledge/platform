{
	"$schema": "https://schemas.wp.org/wp/6.2/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#fff7e5",
					"name": "Cream",
					"slug": "custom-cream"
				},
				{
					"color": "#185500",
					"name": "Dark Green",
					"slug": "custom-dark-green"
				},
				{
					"color": "#fea9be",
					"name": "Pink",
					"slug": "custom-pink"
				},
				{
					"color": "#8ae167",
					"name": "Light Green",
					"slug": "custom-light-green"
				},
				{
					"color": "#ffa81a",
					"name": "Orange",
					"slug": "custom-orange"
				}
			]
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"blockGap": null,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Styrene B Black Trial",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/styrene-b-black-trial_normal_900.otf"
							]
						},
						{
							"fontFamily": "Styrene B Black Trial",
							"fontStyle": "italic",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/styrene-b-black-trial_italic_900.otf"
							]
						}
					],
					"fontFamily": "Styrene B Black Trial",
					"slug": "styrene-b-black-trial"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/columns": {
				"spacing": {
					"padding": {
						"bottom": "16px",
						"left": "16px",
						"right": "16px",
						"top": "16px"
					}
				}
			}
		},
		"color": {
			"background": "var:preset|color|custom-cream",
			"text": "var:preset|color|custom-dark-green"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|styrene-b-black-trial"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "16px",
				"left": "16px",
				"right": "16px",
				"top": "16px"
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}